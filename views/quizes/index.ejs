<table>
	<% var i; for (i=0; i< quizes.length; i++){ %>
		<tr>
			<td>
				<a href="quizes/<%= quizes[i].id %>">
					<%= quizes[i].pregunta%>
				</a>
			</td>
		</tr>
	<% } %>
</table>
<form method="get" action="/quizes">
	<p>Pregunta:</p>
	<input type="text" name="search" value="Responda aqui"/>
	<input type="submit" value="Enviar"/>
</form>
